{
  "expo": {
    "name": "P2Apps",
    "slug": "p2apps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Fink.p2apps",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app.png",
        "backgroundColor": "#c0f4bfff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.Fink.p2apps"
    },
    "web": {
      "favicon": "./assets/images/app.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "5f8dc83d-1c1d-4728-86f7-1fd173584051"
      }
    },
    "owner": "finkdev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5f8dc83d-1c1d-4728-86f7-1fd173584051"
    }
  }
}
